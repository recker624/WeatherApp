{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "quotes": ["error", "double"],
        "no-multi-spaces": ["error"],
        "semi": [2, "always"],
        "brace-style": ["error", "allman"]
    }
}
